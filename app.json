{
    "expo": {
        "name": "Sales & Invoice",
        "slug": "collections",
        "scheme": "collections",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.anonymous.collections"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router"
        ],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "301dae19-6157-4f1e-9f2a-90702eec620e"
            }
        }
    }
}